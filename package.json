{
  "private": true,
  "name": "helloworld-vue",
  "version": "0.1.0",
  "engines": {
    "node": "^8.0.0 || ^10.0.0",
    "npm": "^5.0.0 || ^6.0.0"
  },
  "scripts": {
    "dev": "vue-cli-service serve",
    "lint": "vue-cli-service lint",
    "lint:custom": "eslint --ext .js,.vue ./",
    "lint:critical": "eslint --no-eslintrc --config .eslintrc.critical.json ./src",
    "format": "prettier './**/*.{md,json,js,css,scss,vue}' --write && eslint -o /dev/null --fix --ext .js,.vue ./ || true",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "test": "npm run test:unit && npm run test:e2e",
    "build": "vue-cli-service build"
  },
  "dependencies": {
    "helloworld-lib": "git+https://github.com/emiketic/helloworld-lib.git#0.11.0",
    "portal-vue": "~2.1.0",
    "pwacompat": "~2.0.8",
    "register-service-worker": "~1.6.2",
    "vue": "~2.6.10",
    "vue-router": "~3.0.5",
    "vuetify": "~1.5.11",
    "vuex": "~3.1.0",
    "vuex-persistedstate": "~2.5.4",
    "vuex-router-sync": "~5.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~3.6.0",
    "@vue/cli-plugin-e2e-nightwatch": "~3.6.0",
    "@vue/cli-plugin-eslint": "~3.6.0",
    "@vue/cli-plugin-pwa": "~3.6.0",
    "@vue/cli-plugin-unit-jest": "~3.6.0",
    "@vue/cli-service": "~3.6.0",
    "@vue/eslint-config-airbnb": "~4.0.0",
    "@vue/test-utils": "~1.0.0-beta.25",
    "babel-jest": "~24.7.1",
    "eslint": "~5.16.0",
    "eslint-config-airbnb-base": "~13.1.0",
    "eslint-loader": "~2.1.2",
    "eslint-plugin-import": "~2.17.1",
    "eslint-plugin-vue": "~5.2.2",
    "node-sass": "~4.11.0",
    "prettier": "~1.17.0",
    "sass-loader": "~7.1.0",
    "stylus": "~0.54.5",
    "stylus-loader": "~3.0.2",
    "vue-template-compiler": "~2.6.10"
  }
}
